{
  "name": "boomtzy",
  "version": "1.0.0",
  "description": "A music discord bot",
  "main": "dist/index.js",
  "scripts": {
    "dev:backend": "ts-node-dev --project tsconfig.backend.json -r dotenv/config src/main.ts",
    "dev:frontend": "vue-tsc --project tsconfig.frontend.json && vite build --watch --emptyOutDir",
    "build:frontend": "vue-tsc --project tsconfig.frontend.json && vite build --emptyOutDir",
    "build:backend": "tsc --project tsconfig.backend.json",
    "start": "node -r dotenv/config dist/src/main.js",
    "test": "jest tests/createAudioResource.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Beriu/boomtzy.git"
  },
  "keywords": [
    "discord",
    "bot",
    "music"
  ],
  "author": "Tiberiu Rosu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Beriu/boomtzy/issues"
  },
  "homepage": "https://github.com/Beriu/boomtzy#readme",
  "dependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@discordjs/builders": "^1.3.0",
    "@discordjs/opus": "^0.8.0",
    "@discordjs/rest": "^1.3.0",
    "@discordjs/voice": "^0.13.0",
    "babel-jest": "^28.1.1",
    "bufferutil": "^4.0.6",
    "discord-api-types": "^0.37.12",
    "discord.js": "^14.5.0",
    "dotenv": "^16.0.1",
    "erlpack": "github:discord/erlpack",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.1.0",
    "libsodium-wrappers": "^0.7.10",
    "socket.io": "^4.5.4",
    "ts-jest": "^28.0.5",
    "utf-8-validate": "^5.0.9",
    "vue": "^3.2.45",
    "ytdl-core": "^4.11.2",
    "ytsr": "^3.8.0",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.42",
    "@vitejs/plugin-vue": "^4.0.0",
    "jest": "^28.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3",
    "vite": "^4.0.4",
    "vue-tsc": "^1.0.24"
  }
}
